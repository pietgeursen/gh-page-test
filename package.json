{
  "name": "gh-page-test",
  "version": "1.0.0",
  "description": "c3 demo",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katiewright26/gh-page-test"
  },
  "author": "katiewright26",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/katiewright26/gh-page-test/issues"
  },
  "homepage": "https://github.com/katiewright26/gh-page-test",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "c3": "^0.4.11",
    "gh-pages": "^0.11.0",
    "uglifyify": "^3.0.1"
  }
}
